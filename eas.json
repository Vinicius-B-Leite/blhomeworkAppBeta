{
	"cli": {
		"version": ">= 8.0.0"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"channel": "development",
			"env": {
				"APP_ENV": "development"
			}
		},
		"preview": {
			"distribution": "internal",
			"android": {
				"buildType": "apk"
			},
			"env": {
				"APP_ENV": "preview"
			},
			"channel": "preview"
		},
		"production": {
			"channel": "production",
			"env": {
				"APP_ENV": "production"
			}
		}
	},
	"submit": {
		"production": {}
	}
}
